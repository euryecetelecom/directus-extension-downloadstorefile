{
	"name": "directus-extension-operation-downloadstorefile",
	"description": "Directus extension used to add a download and store file operation to flows",
	"icon": "extension",
	"license": "MIT",
	"version": "1.0.0",
	"author": "Euryece Telecom",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-operation"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.ts",
			"api": "src/api.ts"
		},
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "11.0.1",
		"@types/node": "^20.11.25",
		"release-it": "^17.1.1",
		"typescript": "^5.4.2",
		"vue": "^3.4.21"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/euryecetelecom/directus-extension-operation-download-store-file.git"
	},
	"homepage": "https://github.com/euryecetelecom/directus-extension-operation-download-store-file",
	"release-it": {
		"github": {
			"release": true
		},
		"npm": {
			"publish": true
		}
	},
	"publishConfig": {
    "access": "public"
  }
}
